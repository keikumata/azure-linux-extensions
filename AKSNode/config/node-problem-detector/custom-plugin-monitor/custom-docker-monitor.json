{
    "plugin": "custom",
    "pluginConfig": {
      "invoke_interval": "30s",
      "timeout": "5s",
      "max_output_length": 80,
      "concurrency": 3,
      "enable_message_change_based_condition_update": false
    },
    "source": "docker-custom-plugin-monitor",
    "metricsReporting": true,
    "conditions": [
      {
        "type": "DockerProblem",
        "reason": "DockerIsUp",
        "message": "docker service is up"
      }
    ],
    "rules": [
      {
        "type": "temporary",
        "reason": "DockerIsDown",
        "path": "/etc/node-problem-detector.d/plugin/check_docker.sh",
        "timeout": "3s"
      },
      {
        "type": "permanent",
        "condition": "DockerProblem",
        "reason": "DockerIsDown",
        "path": "/etc/node-problem-detector.d/plugin/check_docker.sh",
        "timeout": "3s"
      }
    ]
  }
  